public class Main {
    static void subseq(String p, String up, int n ) {
        if (up.isEmpty()) {
            System.out.println(p);
            return;
        }
        char ch = up.charAt(0);
        if(p.length()<n)
    {    subseq(p + ch, up,n);} 
        subseq(p, up.substring(1),n); 
    }

    public static void main(String[] args) {
        String input = "abc"; // Example input
        subseq("", input, input.length());
    }
}
